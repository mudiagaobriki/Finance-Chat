{
  "expo": {
    "name": "Parrot",
    "slug": "Parrot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Parrot to access your contacts."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Parrot to access your gallery.",
          "cameraPermission": "Allow Parrot to access your camera."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.parrot.Parrot"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fad84253-4112-4164-a156-c452dc230ac7"
      }
    }
  }
}
